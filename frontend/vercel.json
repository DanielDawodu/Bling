{
    "version": 2,
    "routes": [
        {
            "handle": "filesystem"
        },
        {
            "src": "/api/(.*)",
            "dest": "https://bling-backend-v2.vercel.app/api/$1"
        },
        {
            "src": "/uploads/(.*)",
            "dest": "https://bling-backend-v2.vercel.app/uploads/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}