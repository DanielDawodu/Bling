{
    "version": 2,
    "cleanUrls": true,
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "https://bling-backend-v2.vercel.app/api/$1"
        },
        {
            "source": "/uploads/(.*)",
            "destination": "https://bling-backend-v2.vercel.app/uploads/$1"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}